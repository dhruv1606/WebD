<%- include('partials/head') %>
<body>
  <%- include('partials/navbar') %>
  <h1>Your random number is : <%= num %></h1>

  <% if(num%2 === 1) { %>
    <h2>It is an odd number</h2>
  <% } else { %>
    <h2>It is an even number</h2>
  <% } %>

  <h3><%= num%2===0 ? 'EVEN': 'ODD' %></h3>
</body>
</html>